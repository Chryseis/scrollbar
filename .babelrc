{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [
            "Firefox >= 20",
            "Safari >= 6",
            "Explorer >= 9",
            "Chrome >= 12",
            "ChromeAndroid >= 4.0",
            "iOS >= 6"
          ]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    [
      "@yylc/babel-plugin-demand-loading",
      {
        "library": "@yylc/kit"
      },
      "syntax-decorators"
    ],
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-runtime",
    "transform-es2015-modules-commonjs"
  ]
}